-- Obfuscated Game list with Game IDs (PlaceIds) and corresponding script URLs
local _0x3f8a = {
    [100902716985467] = "https://raw.githubusercontent.com/lumecraftor1/ScriptHub/main/Scripts/refs/heads/main/bee_master_simulator"  -- Bee Master Simulator
}

-- Obfuscated function for checking the current game
local _0x9b7e = game.PlaceId
local _0x763f = _0x3f8a[_0x9b7e]

if _0x763f then
    -- Notify the player
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Univers Hub Notification",
        Text = "Game found! The script is loading.",
        Duration = 5
    })
    
    -- Load and execute the script
    local _0x7c6a, _0x5b4c = pcall(function()
        local _0x4876 = game:HttpGet(_0x763f)
        loadstring(_0x4876)()
    end)
    
    if not _0x7c6a then
        -- If there was an error loading the script
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Error",
            Text = "Failed to load the script.",
            Duration = 5
        })
    end
else
    -- If the game is not found in the list
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Error",
        Text = "Game script not found in the list.",
        Duration = 5
    })
